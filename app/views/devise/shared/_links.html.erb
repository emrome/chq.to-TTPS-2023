<%- if controller_name != 'sessions' %>
  <p class="mt-3"> 
    Already have an account?
    <a href= <%= new_session_path(resource_name) %> >Sign in</a>
    <br/>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <p class="mt-3"> 
    Don't have an account?
    <a href= <%= new_registration_path(resource_name) %> >Sign up</a>
    <br/>
  </p>
<% end %>